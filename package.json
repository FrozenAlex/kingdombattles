{
  "name": "kingdombattles",
  "version": "1.0.0",
  "description": "Kingdom Battles",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently npm:watch:server npm:watch:client",
    "build": "npm run webpack-production",
    "forever": "forever -a -o forever.log -e error.log start server/index.js",
    "restart": "forever -a -o forever.log -e error.log restart server/index.js",
    "stop": "forever stop server/index.js",
    "node": "node server/index.js",
    "webpack": "webpack --mode=development --env=development  --config webpack.dev.js",
    "webpack-production": "webpack --mode=production --env=production  --config webpack.prod.js",
    "db:create": "node scripts/runsql.js createdb.sql",
    "db:drop": "node scripts/runsql.js dropall.sql",
    "pull": "git pull && npm run webpack-production && npm restart",
    "watch:server": "nodemon --watch server server/index.js",
    "watch:client": "webpack-dev-server --env=development  --config webpack.dev.js"
  },
  "author": "Kayne Ruse",
  "license": "",
  "browserslist": [
    "> 2%",
    "not IE <=11"
  ],
  "sideEffects": [
    "*.css"
  ],
  "prettier": {
    "useTabs": true,
    "printWidth": 90,
    "endOfLine": "auto",
    "tabWidth": 2
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "concurrently": "^5.0.2",
    "cron": "^1.7.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-static-gzip": "^2.0.6",
    "firstline": "^2.0.2",
    "forever": "^2.0.0",
    "formidable": "^1.2.1",
    "markdown-to-jsx": "^6.10.3",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "query-string": "^6.11.1",
    "raw-loader": "^4.0.0",
    "sendmail": "^1.4.1",
    "unistore": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "preact": "^10.3.0",
    "preact-async-route": "^2.2.1",
    "preact-router": "^3.2.1",
    "proptypes": "^1.1.0",
    "react-ga": "^2.5.7",
    "react-markdown": "^4.0.8",
    "style-loader": "^1.1.3",
    "svg-inline-loader": "^0.8.0",
    "tailwindcss": "^1.1.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.1"
  }
}
