{
  "name": "kingdombattles",
  "version": "1.0.0",
  "description": "Kingdom Battles",
  "main": "index.js",
  "scripts": {
    "start": "forever -a -o forever.log -e error.log start server/index.js",
    "restart": "forever -a -o forever.log -e error.log restart server/index.js",
    "stop": "forever stop server/index.js",
    "node": "node server/index.js",
    "webpack": "webpack --mode=development --env=development",
    "webpack-production": "webpack --mode=production --env=production",
    "clienttest": "node ./node_modules/karma/bin/karma start karma.conf.js",
    "servertest": "nyc jasmine",
    "fulltest": "npm run servertest && npm run clienttest",
    "pull": "git pull && npm run webpack-production && npm restart"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "include": [
      "server/**/*.js*",
      "spec/**/*.js*"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "report-dir": "coverage/server"
  },
  "author": "Kayne Ruse",
  "license": "",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cron": "^1.7.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "firstline": "^2.0.2",
    "forever": "^1.0.0",
    "formidable": "^1.2.1",
    "mysql": "^2.17.1",
    "query-string": "^6.7.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-loadable": "^5.5.0",
    "react-markdown": "^4.0.8",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-thunk": "^2.3.0",
    "sendmail": "^1.4.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "devDependencies": {
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "nyc": "^14.1.1",
    "puppeteer": "^1.17.0",
    "rewiremock": "^3.13.7"
  }
}
